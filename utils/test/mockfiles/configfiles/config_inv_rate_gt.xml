<?xml version="1.0" ?>
<SemanticEncoder>
	<general>
		<console>True</console>
	</general>
	<auto_encoder>
		<epochs>5</epochs>
		<noise_factor>0.1</noise_factor>
		<encoding_size>128</encoding_size>
	</auto_encoder>
	<semantic_features>
		<noise_rate>1.1</noise_rate>
		<chosen_classes>50, 9, 7, 31, 38</chosen_classes>
		<classes_names>dolphin, blue+whale, horse, giraffe, zebra</classes_names>
	</semantic_features>
	<paths>
		<results_path>_files/results/</results_path>
		<features_path>_files/mockfiles/awa2features/</features_path>
	</paths>
</SemanticEncoder>